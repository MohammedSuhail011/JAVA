package exam;
class adder{
	int add (int a,int b) {	
	  return a+b;}

    double add(double a, double b) {
    return a+b;
    }
    String add(String a, String b) {
    	return a+b;
    }
 }
public class overloadDemo {
       public static void main (String[]args) {
    	   adder obj = new adder();
    	   System.out.println("Sum of integers: " + obj.add(5, 10));
    	   System.out.println("Sum of double: " + obj.add(2.5, 3.7));
    	   System.out.println("Sum of concatenation: " + obj.add("Hello", "World"));
       }
}
